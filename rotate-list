# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution(object):
    def rotateRight(self, head, k):
        """
        :type head: ListNode
        :type k: int
        :rtype: ListNode
        """
        curr_val = []
        curr_add =[]
        curr = head
        while curr!=None:
            curr_add.append(curr)
            curr_val.append(curr.val)
            curr = curr.next
        if len(curr_val) == 0:
            return head
        k = k % len(curr_val)
        for i in range(0,k):
            tail = curr_add[-1]
            h = curr_add[0]
            tail.next = h
            p = curr_add.pop(-1)
            curr_add.insert(0,p)
            tail = curr_add[-1]
            tail.next = None
        return curr_add[0]
