class Solution(object):
    def lengthOfLastWord(self, s):
        len_last_word = 0
        j = len(s) - 1
        
        while j >= 0 and s[j] == ' ':
            j -= 1
        
        while j >= 0 and s[j] != ' ':
            len_last_word += 1
            j -= 1

        return len_last_word




        
